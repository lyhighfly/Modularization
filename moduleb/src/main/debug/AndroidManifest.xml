<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.liukuai.moduleb">

    <application
        android:name=".ModuleBApplication"
        android:allowBackup="true"
        android:icon="@mipmap/moduleb_ic_launcher"
        android:label="@string/moduleb_app_name"
        android:roundIcon="@mipmap/moduleb_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ModuleBMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>